###############################################################################
#                                                                             #
#                Configuration file for the SvxLink server                    #
#                                                                             #
###############################################################################

[GLOBAL]
MODULE_PATH=/usr/lib/svxlink
LOGICS=RepeaterLogic
CFG_DIR=svxlink.d
TIMESTAMP_FORMAT="%c"
#CARD_SAMPLE_RATE=16000
#LOCATION_INFO=LocationInfo

[SimplexLogic]
TYPE=Simplex
RX=Rx1
TX=Tx1
MODULES=ModuleHelp,ModuleParrot,ModuleEchoLink,ModuleTclVoiceMail
CALLSIGN=MYCALL
SHORT_IDENT_INTERVAL=60
LONG_IDENT_INTERVAL=60
#IDENT_ONLY_AFTER_TX=4
#EXEC_CMD_ON_SQL_CLOSE=500
EVENT_HANDLER=/usr/share/svxlink/events.tcl
DEFAULT_LANG=en_US
RGR_SOUND_DELAY=0
REPORT_CTCSS=136.5
#TX_CTCSS=ALWAYS
MACROS=Macros
#LINKS=LinkToR4
FX_GAIN_NORMAL=0
FX_GAIN_LOW=-12
#ACTIVATE_MODULE_ON_LONG_CMD=4:EchoLink
#QSO_RECORDER_DIR=/var/spool/svxlink/qso_recorder
#QSO_RECORDER_CMD=8

[RepeaterLogic]
TYPE=Repeater
RX=MultiRx
TX=MultiTx
#MODULES=ModuleHelp,ModuleParrot,ModuleEchoLink,ModuleTclVoiceMail
MODULES=ModuleHelp
CALLSIGN=YM1C
SHORT_IDENT_INTERVAL=10
LONG_IDENT_INTERVAL=60
IDENT_ONLY_AFTER_TX=4
#EXEC_CMD_ON_SQL_CLOSE=500
EVENT_HANDLER=/usr/share/svxlink/events.tcl
DEFAULT_LANG=en_US
RGR_SOUND_DELAY=0
#REPORT_CTCSS=136.5
#TX_CTCSS=SQL_OPEN
MACROS=Macros
#SEL5_MACRO_RANGE=03400,03499
#LINKS=LinkToR4
FX_GAIN_NORMAL=0
FX_GAIN_LOW=-12
#QSO_RECORDER_DIR=/var/spool/svxlink/qso_recorder
#QSO_RECORDER_CMD=8
#NO_REPEAT=1
IDLE_TIMEOUT=1
#OPEN_ON_1750=1000
#OPEN_ON_CTCSS=136:2000
#OPEN_ON_DTMF=*
OPEN_ON_SQL=50
#OPEN_ON_SEL5=01234
OPEN_SQL_FLANK=OPEN
#OPEN_ON_SQL_AFTER_RPT_CLOSE=10
IDLE_SOUND_INTERVAL=0
#SQL_FLAP_SUP_MIN_TIME=1000
#SQL_FLAP_SUP_MAX_COUNT=10
#ACTIVATE_MODULE_ON_LONG_CMD=4:EchoLink
#IDENT_NAG_TIMEOUT=15
#IDENT_NAG_MIN_TIME=2000

#[LinkToR4]
#CONNECT_LOGICS=RepeaterLogic,SimplexLogic
#COMMAND=94
#NAME=TEST

[Macros]
1=EchoLink:9999#
9=Parrot:0123456789#
03400=EchoLink:9999#

[MultiRx]
TYPE=Voter
#RECEIVERS=Istanbul,Onur,Akin
RECEIVERS=Rx1,Local
VOTING_DELAY=200
#BUFFER_LENGTH=0

[MultiTx]
TYPE=Multi
#TRANSMITTERS=Istanbul,Onur,Akin
TRANSMITTERS=Tx1,Local

[Local]
TYPE=Net
HOST=localhost
TCP_PORT=5220
AUTH_KEY="test"
CODEC=GSM

[Istanbul]
TYPE=Net
HOST=192.168.4.87
TCP_PORT=5220
AUTH_KEY="test"
CODEC=GSM
#SPEEX_ENC_FRAMES_PER_PACKET=4
#SPEEX_ENC_QUALITY=4
#SPEEX_ENC_BITRATE=15000
#SPEEX_ENC_COMPLEXITY=2
#SPEEX_ENC_VBR=0
#SPEEX_ENC_VBR_QUALITY=4
#SPEEX_ENC_ABR=15000
#SPEEX_DEC_ENHANCER=1

[Onur]
TYPE=Net
HOST=192.168.4.105
TCP_PORT=5220
AUTH_KEY="test"
CODEC=GSM

[Akin]
TYPE=Net
HOST=192.168.4.213
TCP_PORT=5220
AUTH_KEY="test"
CODEC=GSM

[Rx1]
